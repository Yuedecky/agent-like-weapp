@import "/assets/wxss/common.wxss";
.address-template-container{
  width: 100%;
  height: 100%;
  padding: 0 20rpx;
}

.address-template-header{
  font-size: 40rpx;
  font-weight: bold;
  height: 60rpx;
  line-height: 60rpx;
}

.address-template-body{
  font-size: 32rpx;
  line-height: 60rpx;
  height: 60rpx;
}

.address-detail{
  width: 80%;
  height: 120rpx;
  border-right: 1px solid gray;
}

.acttion-sheet{
  width: 20%;
  color: #666;
  font-size: 30rpx;
  line-height: 120rpx;
  height: 120rpx;
  text-align: center;
}
